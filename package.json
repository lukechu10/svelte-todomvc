{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "format": "prettier --write src --plugin-search-dir=. ./**/*.html",
    "format:check": "prettier --check src --plugin-search-dir=. ./**/*.html"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.4.0",
    "rollup": "^3.29.5",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte": "^4.2.19",
    "uuid": "^8.3.1"
  }
}